\uc0\u45796 \u51020 \u51004 \u47196  \u44396 \u54788 \u49828 \u53469 \u50640  \u45824 \u54644  \u49444 \u47749 \u54616 \u44192 \u49845 \u45768 \u45796 . \u50500 \u54252 \u47532 \u50880 \u51008  \u47532 \u50529 \u53944 \u50752  \u45432 \u46300 \u51228 \u51060 \u50640 \u49828 \u47484  \u44592 \u48152 \u51004 \u47196  \u44396 \u52629 \u46121 \u45768 \u45796 . \u47532 \u50529 \u53944 \u45716  \u49324 \u50857 \u51088  \u51064 \u53552 \u54168 \u51060 \u49828 \u50752  \u52980 \u54252 \u45324 \u53944  \u44592 \u48152  \u44396 \u51312 \u47484  \u53685 \u54644  \u54952 \u50984 \u51201 \u51064  \u44060 \u48156 \u51012  \u51648 \u50896 \u54616 \u47728 , \u45432 \u46300 \u51228 \u51060 \u50640 \u49828 \u45716  \u48708 \u46041 \u44592  \u52376 \u47532 \u50752  \u54869 \u51109 \u49457  \u45909 \u48516 \u50640  \u48177 \u50644 \u46300  \u49436 \u48260  \u50868 \u50689 \u50640  \u51201 \u54633 \u54633 \u45768 \u45796 . \u46608 \u54620  \u47532 \u50529 \u53944  \u46972 \u50864 \u53552  \u46036 \u51012  \u49324 \u50857 \u54644  \u46041 \u51201  \u54168 \u51060 \u51648  \u51204 \u54872 \u51012  \u44396 \u54788 \u54616 \u50668  \u49324 \u50857 \u51088  \u44221 \u54744 \u51012  \u44060 \u49440 \u54624  \u44228 \u54925 \u51077 \u45768 \u45796 .\
